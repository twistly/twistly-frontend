{
    "name": "Twistly-Frontend",
    "version": "0.0.1",
    "scripts": {
        "test": "xo && nyc ava",
        "dev": "NODE_ENV='development' webpack-dev-server --inline",
        "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
        "update:snapshots": "ava --update-snapshots",
        "postinstall": "npm run build"
    },
    "dependencies": {
        "ava": "^0.19.1",
        "axios": "^0.16.1",
        "axios-cache-plugin": "^0.0.7",
        "babel-cli": "^6.24.1",
        "babel-core": "^6.0.0",
        "babel-loader": "^7.1.1",
        "babel-plugin-syntax-async-functions": "^6.13.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-es2015": "^6.0.0",
        "babel-preset-minify": "^0.2.0",
        "babel-preset-stage-0": "^6.24.1",
        "browser-env": "^2.0.31",
        "bson": "^1.0.4",
        "chart.js": "^2.5.0",
        "compression-webpack-plugin": "^0.4.0",
        "cross-env": "^1.0.6",
        "css-loader": "^0.28.4",
        "dot-prop": "^4.1.1",
        "eslint-plugin-vue": "^2.1.0",
        "git-rev": "^0.2.1",
        "localStorage-mock": "^1.0.3",
        "localstorage-polyfill": "^1.0.1",
        "loglevel": "^1.4.1",
        "numeral": "^2.0.6",
        "require-extension-hooks": "^0.2.0",
        "require-extension-hooks-babel": "^0.1.0",
        "require-extension-hooks-vue": "^0.2.1",
        "stats-lite": "^2.1.0",
        "transform-runtime": "^0.0.0",
        "vue": "^2.3.2",
        "vue-async-computed": "^3.2.1",
        "vue-awesome": "^2.3.1",
        "vue-chartjs": "^2.6.3",
        "vue-hot-reload-api": "^2.1.0",
        "vue-i18n": "^6.1.1",
        "vue-js-modal": "^1.2.2",
        "vue-loader": "12.2.1",
        "vue-resource": "^1.0.3",
        "vue-router": "^2.1.1",
        "vuex": "^2.3.1",
        "vuex-i18n": "^1.3.4",
        "webpack": "2.6.1",
        "webpack-bundle-analyzer": "^2.8.2",
        "webpack-dev-server": "^2.7.1",
        "webpack-s3-uploader": "^1.1.5"
    },
    "devDependencies": {
        "nyc": "^10.3.0",
        "xo": "^0.18.1"
    },
    "xo": {
        "space": 4,
        "rules": {
            "space-before-function-paren": [
                "error",
                "never"
            ],
            "no-console": "error",
            "unicorn/catch-error-name": [
                "error",
                {
                    "name": "error"
                }
            ]
        },
        "envs": [
            "browser",
            "es6"
        ],
        "plugins": [
            "vue"
        ],
        "extensions": [
            "js",
            "vue"
        ]
    },
    "ava": {
        "babel": "inherit",
        "require": [
            "./test/helpers/setup.js"
        ]
    },
    "nyc": {
        "extension": [
            ".js",
            ".vue"
        ]
    }
}
